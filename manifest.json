{
  "manifest_version": 2,
  "name": "Ex",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [
        "hyperapp.js", 
        "jquery.min.js", 
        "jquery.unveil.js",
        "content.js"
      ],
      "css": ["styles.css"]
    }
  ]
}
